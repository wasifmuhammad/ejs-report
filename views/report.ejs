    <img src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/ps-logo.png" />
  </div>
  <div class="main-container">
    <div class="page-1 page">

      <div class="header">
        <p class="w7">Assessment Summary Report</p>
        <div class="flex-row space-between pt15">
          <div class="logo-container flex-row">
            <img class="logo" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/logo.png" />
            <div class="flex-col">
              <img class="prevent-scripts" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/prevent-scripts.png" />
              <div class="title pt5"></div>
              <p class="UP pt5 text w3">prevention intervention</p>
              <p class="UP text w3">recommendation</p>
            </div>
          </div>
          <div class="patient-details-container flex-row space-between">
            <div class="titles w6">
              <p>Patient Full Name</p>
              <p>Date of Birth</p>
              <p>Provider</p>
              <p>Hospital</p>
              <p>Date of Report</p>
            </div>
            <div class="values">
              <p><%= reqData?.firstName %> <%= reqData?.lastName %></p>
              <p><%= formattedDOB %></p>
              <p><%= doctorData?.firstName %> <%= doctorData?.lastName %></p>
              <p><%= doctorData?.hospital %></p>
              <p><%= dateSubmitted %></p>
            </div>
          </div>
        </div>
        <div class="labs-info w7 mt5">
          These labs may be of clinical benefit: A1e, CMP, Cholesterol Panel,
          Fasting insulin
        </div>
      </div>
      <div class="profiles-container flex-row space-between pt20 start">
        <div class="risk-profile">
          <p class="heading w7">Risk Profile</p>
          <div class="data-container mt5">
            <div class="start flex-row space-between">
              <div style="width: 100%;">
                <div class="flex-row space-between">
                  <div class="points">
                    <div class="point flex-row start">
                      <img class="downIcon" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/downIcon.png" />
                      <p>Pre-diabetes Risk:
                      <p class="pl2 w7 rating"><%= riskRating %></p>
                      </p>
                    </div>
                    <div class="point flex-row start">
                      <img class="downIcon" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/downIcon.png" />

                      <div class="flex-row align-center">
                        <p>Patient Reported BMI: </p>
                        <p><%= reqData?.bmi%></p>
                      </div>
                    </div>

                    <div class="point flex-row start">
                      <img class="downIcon" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/downIcon.png" />

                      <div class="flex-row align-center">
                        <p>Smoking Status:
                        <p><%= reqData?.smoker ? 'is a smoker' : 'not a smoker'%></p>
                        </p>

                      </div>
                    </div>

                  </div>
                </div>
              </div>
              <div class="score-container">
                <div class="flex-row text-container-1 w5">
                  <p>Score:</p>
                  <p class="pl2 red"><%= riskScoring %></p>
                  <p class="pl2">/</p>
                  <p class="pl2">10</p>
                </div>
                <div class="flex-row text-container-2">
                  <p class="UP w7 no-wrap"><%= riskRating %></p>
                  <div class="flex-container">
                    <% for(var i =0; i< 10; i++) { %>

                    <div>
                      <% if (i < riskScoring) { %>
                      <div class="flex-item score-item-<%= i %>"></div>
                      <% } else { %>
                      <div class="flex-item gray"></div>
                      <% } %>
                    </div>

                    <% } %>
                  </div>
                </div>
              </div>
            </div>
            <div class="points">





              <div class="point flex-row start pt3">
                <img class="downIcon" src="https://preventscripts-bucket.s3.eu-west-3.amazonaws.com/icons/downIcon.png" />

                <div class="flex-row align-center">
                  <p>Patient reported Chronic Conditions:
                  <p><%= [...chronicConditions].join(", ") %></p>
                  </p>

                </div>
              </div>
            </div>
            <div class="recommendation-container pt30">
              <p class="w7 title">PreventScripts Program Recommendations</p>
              <div class="flex-row start pt2">
                <div class="yellow-point"></div>
                <p class="sub-text pl2"><%= rpmRecommendations %></p>
              </div>
            </div>
          </div>
        </div>
        <div class="behaviour-profile">
          <p class="heading w7">Behaviour Profile</p>
          <div class="data-container mt5">
            <div class="promis pt2">
              <p class="sub-heading">
